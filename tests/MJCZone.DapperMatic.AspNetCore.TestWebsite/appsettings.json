{
  "DapperMatic": {
    "BasePath": "api/ddl-tests",
    "RequireAuthentication": false,
    "Datasources": [
      // Add another datasource here for SQLite that automatically creates a file-based database
      {
        "Id": "mySqliteDatasource",
        "Provider": "SQLite",
        // Co-erce SQLite to create a file-based database named "myDatabase.db" in the application root,
        // and to use the System.Data.SQLite ADO.NET provider
        "ConnectionString": "Provider=System.Data.SQLite;Data Source=myDatabase.db;Version=3;",
        "DisplayName": "Sample SQLite Datasource"
      },
      {
        "Id": "mySqliteDatasource2",
        "Provider": "SQLite",
        // Co-erce SQLite to create a file-based database named "myDatabase.db" in the application root,
        // and to use the Microsoft.Data.Sqlite ADO.NET provider
        "ConnectionString": "Provider=Microsoft.Data.Sqlite;Data Source=myDatabase2.db;",
        "DisplayName": "Sample SQLite Datasource (using Microsoft.Data.Sqlite)",
        "Description": "This datasource uses the Microsoft.Data.Sqlite ADO.NET provider"
      },
      {
        "Id": "mySqlDatasource",
        "Provider": "SqlServer",
        "ConnectionString": "Server=localhost;Database=myDb;User Id=myUser;Password=myPass;Connection Timeout=2;",
        "DisplayName": "My Datasource",
        "Description": "A description of my datasource",
        "Tags": ["tag1", "tag2"],
        "IsEnabled": true
      }
    ],
    "ConnectionStringEncryptionKey": "IynMTkTah9YeqrNe0nY1ETtRkkf8PFb1uaZ9lqteeGc="
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
